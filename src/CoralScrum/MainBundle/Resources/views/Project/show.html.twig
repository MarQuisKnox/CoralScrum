{% extends "CoralScrumMainBundle::index.html.twig" %}

{% block container -%}
    <h1>Project</h1>

    <table class="record_properties cleanTab">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Ispublic</th>
                <td>{{ entity.isPublic }}</td>
            </tr>
            <tr>
                <th>Deposit</th>
                <td>{{ entity.deposit }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('project') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('project_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
